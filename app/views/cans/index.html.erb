  <%= include_gon %>
<% unless current_user %> 
  <h1 class="text-center pt-5 pb-1">Welcome to LitterWalk</h1>
    <%= render 'users/login' %>
  <% else %>
    <h1 class="display-4 text-center pt-3 pb-1">Cans near you</h1>
  

    

  <div class="text-center pb-3">
    <%= form_tag searched_path do%>
      <%=select_tag "search_term", options_for_select(['Trash', 'Recycling', 'Pet Waste', 'Organic', 'E-Waste'])%>
      <%= submit_tag "Search", class: "btn btn-primary" %>
    <% end %>
  </div>




    <div id="map"></div>
   
    <script async defer
    src="https://maps.googleapis.com/maps/api/js?key=AIzaSyBwhVnvsCXhml1l6gb3-S9eTzLzSPG1gGg&callback=initMapp">
    </script>
<% end %>
