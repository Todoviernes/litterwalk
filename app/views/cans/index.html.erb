  <%= include_gon %>
<% unless current_user %> 
  <h1 class="text-center pt-5 pb-1">Welcome to LitterWalk</h1>
    <%= render 'users/login' %>
  <% else %>
    <h1 class="display-4 text-center pt-3 pb-1">Cans near you</h1>
  

    <% if flash[:success] %>
      <div class="notice"><%= flash[:success] %></div>
    <% else %>
      <div class="notice"><%= flash[:error] %></div>    
  <% end %>




    <div id="map"></div>
   
    <script async defer
    src="https://maps.googleapis.com/maps/api/js?key=AIzaSyBwhVnvsCXhml1l6gb3-S9eTzLzSPG1gGg&callback=initMapp">
    </script>
<% end %>
